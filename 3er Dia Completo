        "calificaciones = [[100,83,96],[78,56,99]]\n",
        "i = 1\n",
        "for cali_i in calificaciones:\n",
        "  print(f\"/nAlumno {i}\")\n",
        "  j = 1\n",
        "  for cali_j in cali_i:\n",
        "    print(f\" Cali {j}:{cali_j}\")\n",
        "    j += 1\n",
        "  j=0\n",
        "  i += 1"
